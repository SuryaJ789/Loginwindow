<Window x:Class="Login_page_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login_page_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="415" Width="214">
    <Grid Margin="0,0,71,20">
        <Grid.RowDefinitions>
            <RowDefinition Height="106*"/>
            <RowDefinition Height="273*"/>
        </Grid.RowDefinitions>
        <Image x:Name="image" Margin="-115,0,-174,-20" Source="/25-258747_mobile-3d-wallpaper-hd-1080p-free-download.jpg" Grid.RowSpan="2"/>
