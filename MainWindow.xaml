            <TextBox.Background>
                <ImageBrush/>
            </TextBox.Background>
        </TextBox>
        <TextBox x:Name="textBox1" Margin="116,78,-60,-1" TextWrapping="Wrap" FontWeight="Bold" FontSize="14" Foreground="#FFFCFCFC">
            <TextBox.Background>
                <ImageBrush/>
            </TextBox.Background>
        </TextBox>
        <Button x:Name="button" Content="SIGNIN" Margin="80,94,-35,149" Click="button_Click" Grid.Row="1" Foreground="#FF0C27EB" FontWeight="Bold">
            <Button.Background>
                <ImageBrush/>
            </Button.Background>
        </Button>
        <Label x:Name="label1" Content="password" Margin="10,78,71,0" d:LayoutOverrides="HorizontalAlignment" Foreground="#FF85DC16"/>
    </Grid>
</Window>
